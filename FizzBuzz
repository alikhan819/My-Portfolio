while True:
    number = int(input("What is the number: "))

    def multiple():
        if number % 3 == 0 and number % 5 == 0:
            print("Fizz Buzz")
        elif number % 3 == 0:
            print("Fizz")
        elif number % 5 == 0:
            print("Buzz")
        else:
            print('Beeeeeep')
    
    multiple()
    
    dec = input("Do you want to continue?: ").lower()
    
    if dec == 'yes':
        continue
    elif dec == 'no':
        break
print("Byeeee")
